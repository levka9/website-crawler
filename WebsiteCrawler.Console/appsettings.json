{
  "Elasticsearch": {
    "Url": "http://127.0.0.1",
    "Port": "9200",
    "TimeoutInSeconds": 10,
    "DefaultIndex": "website_crawler", //"websiteCrawler",
    "Username": "",
    "Password": ""
  },
  "Parser": {
    "MultithreadParser": {
      "MaxTaskQuantity": "10"
    },
    "WebsiteParse": {
      "MaxDeep": 1,
      "MaxInternalLinks": 30,
      "DomainExtentions": [
        // Israels domain extensions https://en.wikipedia.org/wiki/.il
        "co.il",
        "org.il",
        "net.il",
        "ac.il",
        "k12.il",
        "gov.il",
        "muni.il",
        "idf.il"
      ]
    }
  }
}
